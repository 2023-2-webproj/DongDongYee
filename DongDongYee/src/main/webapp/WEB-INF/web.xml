<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>CheckIdServlet</servlet-name>
        <servlet-class>Controller.CheckIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckIdServlet</servlet-name>
        <url-pattern>/Controller.CheckIdServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>CheckEmailServlet</servlet-name>
        <servlet-class>Controller.CheckEmailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckEmailServlet</servlet-name>
        <url-pattern>/Controller.CheckEmailServlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>CheckNicknameServlet</servlet-name>
        <servlet-class>Controller.CheckNicknameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckNicknameServlet</servlet-name>
        <url-pattern>/Controller.CheckNicknameServlet</url-pattern>
    </servlet-mapping>
</web-app>
